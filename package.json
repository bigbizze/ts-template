    "@types/jest": "^29.5.11",
    "@types/node": "latest",
    "@types/uuid": "^9",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "app-root-path": "^3.1.0",
    "eslint": "^8.25.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-no-null": "^1.0.2",
    "git-user-name": "^2.0.0",
    "jest": "^29.2.0",
